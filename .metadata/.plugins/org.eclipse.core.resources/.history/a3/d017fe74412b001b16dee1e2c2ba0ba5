package random;

import java.util.*;

public class BinaryStringReduction {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int t=0;
		if(sc.hasNextInt())
			t=sc.nextInt();
		
		while(t-->0) {
			
			int n=sc.nextInt();
			sc.nextLine();
			String str=sc.next();
			
			
			LinkedList<Character> list=new LinkedList<>();
			
			for(int i=0;i<n;i++) {
				list.add(str.charAt(i));
			}
			
			int count=0;
			
//			System.out.println(list);
			
			while(list.size()>0)
			{
				
				char p=list.get(0);
				
				if(p=='0')
				{
					
					for(int i=0;i<list.size();i++) {
						
						if(list.get(i)=='0') {
							
							for(int j=i+1;j<list.size();j++) {
								
								if(list.get(j)=='1') {
									
									list.remove(i);
								
									list.remove(j);
									i=j;
									
									break;
									
								}
								
							}
							
						}else {
							continue;
						}
			
					}
					
	     	}else
				{
					for(int i=0;i<list.size();i++) {
						
						if(list.get(i)=='1') {
							
							for(int j=i+1;j<list.size()-1;j++) {
								
								if(list.get(j)=='0') {
									
									list.remove(i);
									list.remove(j);
									i=j;
									
									break;
									
								}
								
							}
							
						}else {
							continue;
						}
						
						
						
						
						
					}
				}
				
				count++;
				
				System.out.println(list);
				
			}
			
			System.out.println(list);
		
			count += list.size();
			
			System.out.println(count);
			
			
			
			
			
			
			
			
			
//			while(list.size()>0)
//			{
//				
//				char p=list.get(0);
//				if(p==target0)
//				{
