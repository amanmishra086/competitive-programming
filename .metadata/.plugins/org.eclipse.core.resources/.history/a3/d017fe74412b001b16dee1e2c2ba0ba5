//					for(int i=1;i<n;i++) {
//						
//						if(target0==null && list.get(i)==taget1) {
//							
//							list.remove(0);
//							list.remove(i);
//							
//							tareget1=null;
//							target0=0;
//							
//						}else if(target1==null && list.get(i)==target0) {
//							
//							target0=null;
//							target1=1;
//							
//						}
//						
//					}
//					
//					
