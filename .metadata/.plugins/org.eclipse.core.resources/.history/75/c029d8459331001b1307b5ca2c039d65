package novemberLongChallenge;

import java.util.Scanner;

public class FRCTNS {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		
			
			int count=0;
			int n=sc.nextInt();
			
			for(int i=1;i<=n;i++) {
				
				for(int j=1;j<=n;j++) {
					
					int p=i*(j+1);
					int q=(i+1)*j;
					
				    
					double f=p/q;
					double k=(q-p)/q;
					
					if((f+k) == 1) {
						count++;
					}
					
					
				}
			}
			
			System.out.println(count);
			
		}
	

}
