package novemberLongChallenge;

import java.util.Scanner;

public class FRCTNS {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		
			
			int count=0;
			int n=sc.nextInt();
			double ans=0;
			for(int i=1;i<=n;i++) {
				
				for(int j=1;j<=n;j++) {
					
					int p=i*(j+1);
					int q=(i+1)*j;
					
				   System.out.println((p/q)+(q-p)/q);
					
				}
			}
			
			//System.out.println(count);
			
		}
	

}
