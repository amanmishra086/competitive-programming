package recusionANDbacktracking;

public class ZeroOneKnapsack {

	public static void main(String[] args) {
		
		int capacity=6;
		int weight[]= {1,2,3,5};
		int price[]= {40,20,35,100};
		
		
		System.out.println(maxprice(capacity,weight,price,0));

	}

	static int maxprice(int cap,int[] w, int[] p,int index) {
		if(index>=w.length) {
			return 1;
		}
		
		if(cap==0) {
			System.out.println("hello");
		}
		
		
		int a=maxprice(cap-w[index], w, p, index+1);
		
		
		
		return a;
	}
}
