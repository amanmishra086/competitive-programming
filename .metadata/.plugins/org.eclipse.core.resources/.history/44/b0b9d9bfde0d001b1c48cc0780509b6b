package recusionANDbacktracking;

import java.util.Arrays;

public class RatInMaze {
 
	public static void main(String[] args) {
		
		String maze[]= { "0000",
						 "00X0",
						 "000X",
						 "0X00" };
		
   	     int sol[][]=new int[10][10];
   	  
   	    
   	 for(int i=0;i<10;i++) {    
   		 for(int j=0;j<10;j++) {
   			 System.out.print(sol[i][j]+" ");
   		 }
   		 System.out.println();
   	 }
   	 
		
		
		
		

	}

	
}
