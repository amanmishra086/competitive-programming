package recusionANDbacktracking;

public class GenerateBracketsusingRecursion {

	public static void main(String[] args) {
		
		int n=20;
		
	 brackets(n,0,0,"");
		
		
	}

	private static void brackets(int n,int open,int close,String ans) {
		
		if(n<0) {
			System.out.println(ans);
			return;
		}
		
		//rec case
		if(open<=n) {
			brackets(n-1,open+1,close,ans+'(');
		}
		 if(close<open) {
			brackets(n-1,open,close+1,ans+')');
		}
		
		
	}

}
